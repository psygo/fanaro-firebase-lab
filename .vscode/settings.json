{
    "files.exclude": {
        //   0. General Stuff
        ".gitignore": false,
        "README.md": false,
        "public/": true,
        //
        //   1. JS | TS | Packages
        "**/dist/": false,
        "**/index.js": false,
        "**/index.js.map": true,
        "**/node_modules/": true,
        "**/package-lock.json": true,
        "**/tsconfig.tsbuildinfo": true,
        //
        //   2. Firebase
        //
        // 2.1. Config
        ".firebase/": true,
        "**/firebase*": false,
        "**/firebase/": false,
        "**/firebase.json": false,
        "**/.firebaserc": false,
        "**/firestore.indexes.json": false,
        "**/firestore.rules": false,
        "**/remoteconfig.template.json": false,
        //
        // 2.2. Debug
        "**/firebase-debug.log": true,
        "**/firestore-debug.log": true,
        "**/pubsub-debug.log": true,
        "**/ui-debug.log": true,
        //
        //   3. Others
        "**/index.js.LICENSE.txt": true,
    }
}